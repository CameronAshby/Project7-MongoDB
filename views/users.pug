div
    p
        a(href='/newUser') Add User
    h1 User List

table
    tr
        th Username
        th FirstName
        th LastName
        th Email
        th Age
        th
        th
    tr
    for user in userArray
        tr
            td= user.userId
            td= user.firstName
            td= user.lastName
            td= user.email
            td= user.age
            td
                a(href='/user/' + user.userId) Edit
            td
                a(href='/removeUser/' + user.userId) Delete
